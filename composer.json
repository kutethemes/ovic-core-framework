{
    "name": "ovic-core/framework",
    "description": "The framework for developing ovic",
    "keywords": [
        "css",
        "js",
        "ovic",
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ovic Team",
            "email": "tf.kutethemes@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Ovic\\Framework\\": "src/"
        },
        "classmap": [
            "database/migrations",
            "database/factories",
            "src/Models"
        ],
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": ">= 5.6",
        "laravel/ui": "*",
        "laravel/valet": "*",
        "doctrine/dbal": "*",
        "ovic-core/user": "*",
        "nwidart/laravel-modules": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ovic\\Framework\\FrameworkServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "6.0-dev"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/kutethemes/ovic-core-framework"
        }
    ],
    "scripts": {
        "post-package-install": [
            "@php artisan ui vue --auth"
        ]
    },
    "support": {
        "issues": "https://kutethemes.com/supports/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
