{
    "name": "ovic-core/framework",
    "description": "The framework for developing ovic",
    "keywords": [
        "css",
        "js",
        "ovic",
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ovic Team",
            "email": "tf.kutethemes@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ovic\\Framework\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": ">= 5.6",
        "nwidart/laravel-modules": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ovic\\Framework\\FrameworkServiceProvider"
            ],
            "dont-discover": [
                "Ovic\\Framework\\FrameworkServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=ovic-assets",
            "@php artisan vendor:publish --tag=ovic-config"
        ]
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/kutethemes/ovic-core-framework"
        }
    ],
    "support": {
        "issues": "https://kutethemes.com/supports/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
